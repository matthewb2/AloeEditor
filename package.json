{
  "name": "Aloe Editor",
  "version": "1.0.0",
  "description": "Monaco based code editor",
  "main": "main.js",
  "build": {
        "appId": "AloeEditor",
    "win": {
      "target": "nsis",
      "icon": "icon.ico"
    }, 
    "files": [
      "**/*",
      "monaco/**/*"
    ],
    "extraResources": [
      {
        "from": "monaco",
        "to": "monaco",
        "filter": [
          "**/*"
        ]
      }
    ]
  },
  "scripts": {
    "start": "electron .", 
    "build": "electron-builder"
  },
  "keywords": [
    "Electron",
    "Hello World"
  ],
  "author": "Your Name",
  "license": "ISC",
  "devDependencies": {
    "electron": "^39.2.4",
    "electron-builder": "^26.0.12"
  }
}
